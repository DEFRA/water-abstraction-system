{
  "name": "water-abstraction-system",
  "version": "0.7.0",
  "description": "System for managing water abstraction licences, including alerts, returns and billing",
  "homepage": "https://github.com/DEFRA/water-abstraction-team",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "bin/build",
    "migrate:make": "knex migrate:make --",
    "migrate:db": "knex migrate:latest",
    "migrate:db:test": "NODE_ENV=test knex migrate:latest",
    "rollback:db": "knex migrate:rollback --all",
    "rollback:db:test": "NODE_ENV=test knex migrate:rollback --all",
    "seed:db": "knex seed:run --knexfile knexfile.application.js",
    "lint": "standard",
    "test": "lab --silent-skips --shuffle",
    "postinstall": "npm run build",
    "version": "npx --yes auto-changelog -p --commit-limit false && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DEFRA/water-abstraction-system"
  },
  "author": "Water service team",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@airbrake/node": "^2.1.8",
    "@aws-sdk/client-s3": "^3.331.0",
    "@hapi/boom": "^10.0.0",
    "@hapi/hapi": "^21.1.0",
    "@hapi/inert": "^7.0.0",
    "@hapi/vision": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "blipp": "^4.0.2",
    "dotenv": "^16.0.3",
    "got": "^12.5.3",
    "govuk-frontend": "^4.4.1",
    "hapi-pino": "^11.0.1",
    "hpagent": "^1.2.0",
    "joi": "^17.7.0",
    "knex": "^2.3.0",
    "nunjucks": "^3.2.3",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
    "sass": "^1.56.2",
    "tar": "^6.1.15"
  },
  "devDependencies": {
    "@hapi/code": "^9.0.2",
    "@hapi/hoek": "^11.0.2",
    "@hapi/lab": "^25.0.1",
    "mock-fs": "^5.2.0",
    "nock": "^13.2.9",
    "pino-pretty": "^9.1.1",
    "proxyquire": "^2.1.3",
    "sinon": "^15.0.1",
    "standard": "^17.0.0"
  }
}
